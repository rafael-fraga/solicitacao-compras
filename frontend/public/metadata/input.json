{
  "name": "brazas-1",
  "component": "ZdContainer",
  "children": [
    {
      "name": "denseForm",
      "component": "ZdForm",
      "children": [
        {
          "name": "returnObjectForm",
          "component": "ZdForm",
          "children": [
            {
              "name": "objectSelectResult",
              "component": "ZdSelect",
              "label": "Select",
              "dataText": "name",
              "dataValue": "value",
              "returnObject": true,
              "datasource": {
                "data": "{{InputController.teste}}"
              },
              "events": {
                "onMounted": "{{InputController.onMounted}}"
              }
            },
            {
              "name": "selected-value",
              "component": "ZdText",
              "text": "{{InputController.selectedItemObject}}",
              "cssClass": "zd-form-input-align"
            }
          ]
        },
        {
          "name": "text-rest-datasource",
          "component": "ZdText",
          "text": "{{SelectController.selectedItemObject}}",
          "cssClass": "zd-form-input-align"
        },
        {
          "name": "tag",
          "component": "ZdSelect",
          "label": "Produto",
          "datasource": {
            "uniqueKey": "ip",
            "route": "/v1/json/",
            "type": "rest",
            "lazyLoad": true,
            "limit": 5,
            "arrayFormat": "repeat"
          }
        },
        {
          "name": "tag",
          "component": "ZdSelect",
          "label": "Fornecedor",
          "datasource": {
            "type": "simple",
            "items": ["ABC", "Super Nosso", "Carrefour", "Super n sei oq"]
          }
        },
        {
          "name": "tag",
          "component": "ZdSelect",
          "label": "Tipo (unidade)",
          "datasource": {
            "type": "simple",
            "items": ["Unidade", "KG", "G"]
          }
        },
        {
          "name": "Prefix",
          "component": "ZdTextInput",
          "label": "Valor",
          "prefix": "R$"
        },
        {
          "name": "divisor",
          "component": "ZdDivisor"
        },
        {
          "name": "footer-basic-usage-ds",
          "component": "ZdFooter",
          "color": "white",
          "rightSlot": [
            {
              "name": "infoButton",
              "component": "ZdButton",
              "label": "Limpar",
              "color": "error"
            },
            {
              "name": "infoButton",
              "component": "ZdButton",
              "label": "Salvar",
              "color": "success"
            }
          ]
        }
      ]
    }
  ]
}
