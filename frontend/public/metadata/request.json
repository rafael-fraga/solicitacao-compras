{
  "name": "raisedCard",
  "component": "ZdCard",
  "raised": true,
  "children": [
    {
      "name": "text",
      "component": "ZdText",
      "text": "Gerar solicitação",
      "justify": "center",
      "align": "center",
      "cssStyle": "padding:0px 0px 20px 0px; font-size: 10px",
      "tag": "h1"
    },
    {
      "name": "button-basic-usage-dsadsa",
      "component": "ZdButton",
      "label": "Preco final: R$ 12.560",
      "color": "alert",
      "absolute": true,
      "right": true
    },
    {
      "name": "grid-inlineEditComps",
      "component": "ZdGridEditable",
      "showFooter": false,
      "gridHeight": "260px",
      "toolbarSlot": [
        {
          "name": "cancelButton-inlineEditComps",
          "component": "ZdButton",
          "iconName": "close",
          "label": "Cancelar",
          "color": "error",
          "flat": true,
          "disabled": "{{GridEditableController.isNotInlineCompsEditing}}",
          "events": {
            "click": "{{GridEditableController.cancelEditingComps}}"
          }
        },
        {
          "name": "saveButton-inlineEditComps",
          "component": "ZdButton",
          "iconName": "complete",
          "label": "Salvar",
          "color": "success",
          "flat": true,
          "disabled": "{{GridEditableController.isNotInlineCompsEditing}}",
          "events": {
            "click": "{{GridEditableController.saveEditingComps}}"
          }
        }
      ],
      "columns": [
        {
          "name": "item",
          "label": "Item",
          "editable": false,
          "minWidth": "150px",
          "componentProps": {
            "component": "ZdText"
          }
        },
        {
          "name": "fornecedor",
          "label": "Fornecedor",
          "editable": false,
          "minWidth": "150px",
          "componentProps": {
            "component": "ZdText"
          }
        },
        {
          "name": "valor",
          "label": "Valor/{unidade}",
          "editable": false,
          "minWidth": "150px",
          "componentProps": {
            "component": "ZdNumber",
            "mask": {
              "currencySymbol": "R$ "
            }
          }
        },
        {
          "name": "quantidade",
          "label": "Quantidade",
          "editable": true,
          "minWidth": "150px",
          "componentProps": {
            "component": "ZdNumber"
          }
        },
        {
          "name": "total",
          "label": "Total",
          "editable": false,
          "minWidth": "150px",
          "componentProps": {
            "component": "ZdNumber",
            "mask": {
              "currencySymbol": "R$ "
            }
          }
        },
        {
          "name": "action3",
          "type": "action",
          "children": [
            {
              "name": "btnTwitter",
              "component": "ZdButton",
              "icon": true,
              "small": true,
              "color": "red",
              "iconName": "mdi-trash-can-outline",
              "conditions": {
                "isVisible": "{{GridController.isTwitterActionVisible}}"
              },
              "events": {
                "click": "{{GridController.buttonAction}}"
              }
            }
          ]
        }
      ],
      "datasource": {
        "uniqueKey": "id",
        "data": [
          {
            "item": "nome do item",
            "fornecedor": "Super nosso",
            "valor": 1500,
            "quantidade": 1,
            "total": 10000
          },
          {
            "item": "nome do item",
            "fornecedor": "ABC",
            "valor": 320,
            "quantidade": 2,
            "total": 3232
          },
          {
            "item": "nome do item",
            "fornecedor": "Super nosso",
            "valor": 420,
            "quantidade": 1,
            "total": 6565
          },
          {
            "item": "nome do item",
            "fornecedor": "ABC",
            "valor": 100,
            "quantidade": 2,
            "total": 121
          },
          {
            "item": "nome do item",
            "fornecedor": "Super nosso",
            "valor": 4200,
            "quantidade": 1,
            "total": 5325
          },
          {
            "item": "nome do item",
            "fornecedor": "ABC",
            "valor": 500,
            "quantidade": 2,
            "total": 54353
          },
          {
            "item": "nome do item",
            "fornecedor": "Super nosso",
            "valor": 900,
            "quantidade": 1,
            "total": 1121
          },
          {
            "item": "nome do item",
            "fornecedor": "ABC",
            "valor": 350,
            "quantidade": 2,
            "total": 45343
          }
        ]
      },
      "events": {
        "inlineEdit": "{{GridEditableController.onInlineEditComps}}"
      }
    },
    {
      "name": "dslsda",
      "component": "ZdDivider",
      "cssStyle": "padding-bottom: 40px"
    },
    {
      "name": "footer-basic-usage-ds",
      "component": "ZdFooter",
      "color": "white",
      "leftSlot": [
        {
          "name": "button-basic-usage-dsadsa",
          "component": "ZdButton",
          "label": "Quantidade final: 5 produtos",
          "color": "hexa color",
          "flat": true
        }
      ],
      "rightSlot": [
        {
          "name": "button-basic-usage-dsa",
          "component": "ZdButton",
          "label": "Salvar solicitação (CSV)",
          "color": "success"
        },
        {
          "name": "button-basic-usage-dsa",
          "component": "ZdButton",
          "label": "Enviar solicitação",
          "color": "success"
        }
      ]
    }
  ]
}
